	<select id="rescomplete_ground_as_date"
		parameterType="ComResInfoDto" resultType="ComResInfoDto">
		select
		RES_DATE,RES_TIME,branch_tb.BRANCH_NO,branch_tb.BRANCH_NAME,ground_tb.GROUND_NO,ground_tb.GROUND_NAME
		from reservation_tb,branch_tb,ground_tb where branch_tb.BRANCH_NO =
