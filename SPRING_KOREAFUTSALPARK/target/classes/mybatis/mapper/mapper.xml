	<select id="datepicker_selecthidden"
		parameterType="ComResInfoDto" resultType="ComResInfoDto">
		select count(*) as radiocount ,ground_tb.GROUND_NO, ground_tb.GROUND_NAME
		from reservation_tb,branch_tb,ground_tb where branch_tb.BRANCH_NO =
		ground_tb.BRANCH_NO and
		reservation_tb.GROUND_NO = ground_tb.GROUND_NO and (RES_STATUS = 10 or RES_STATUS = 20) and
		RES_DATE = #{res_date} and branch_tb.BRANCH_NO = #{branch_no} group by
		ground_tb.GROUND_NO
	</select>
	<select id="fulldate_select" parameterType="ComResInfoDto"
		resultType="ComResInfoDto">
		select count(c.RES_DATE) as fulldate,c.RES_DATE from(select count(*) as
		sevencount,reservation_tb.RES_DATE,ground_tb.GROUND_NO,
		ground_tb.GROUND_NAME from reservation_tb,branch_tb,ground_tb where
		branch_tb.BRANCH_NO = ground_tb.BRANCH_NO and
		reservation_tb.GROUND_NO = ground_tb.GROUND_NO and (RES_STATUS = 10 or RES_STATUS = 20) and
		branch_tb.BRANCH_NO = #{branch_no}
		group by ground_tb.GROUND_NO,reservation_tb.RES_DATE having sevencount = 7)
		as c group by c.RES_DATE having fulldate = #{fulldate}
	</select>
	<update id="oneday_status30" parameterType="ReservationDto">
						<![CDATA[update reservation_tb set RES_STATUS=30 where DATE_ADD(RES_MOMENT, INTERVAL 1 DAY) < now() and RES_STATUS=10]]>
	</update>
