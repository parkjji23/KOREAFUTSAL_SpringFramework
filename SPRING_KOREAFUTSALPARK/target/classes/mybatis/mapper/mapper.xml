		BIRTH, PIC, LEVEL, POSITION, SECOND_PW, SECOND_PWC, BANK_CODE, ACCOUNT_CODE,ACCOUNT_HOLDER)
		values(#{ID},#{NAME},#{PW},#{TEL},#{EMAIL},#{GENDER},#{ADDRESS_CODE},#{ADDRESS_BASIC},#{ADDRESS_DETAIL},
		#{BIRTH},#{PIC},#{LEVEL},#{POSITION},#{SECOND_PW},#{SECOND_PWC},#{BANK_CODE},#{ACCOUNT_CODE},#{ACCOUNT_HOLDER})
