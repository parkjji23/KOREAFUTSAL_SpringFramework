	<sql id="searchCondition_match">
		<if test="searchType != null">
			<if test="searchType == 'contents'.toString()">and M_B_CONTENTS like concat('%', #{keyword}, '%')</if>
			<if test="searchType == 'name'.toString()">and M_B_NAME like concat('%', #{keyword}, '%')</if>
			<if test="searchType == 'branch'.toString()">and M_B_BRANCH like concat('%', #{keyword}, '%')</if>
			<if test="searchType == 'all'.toString()">and (M_B_CONTENTS like concat('%', #{keyword}, '%') or
				M_B_NAME
				like concat('%', #{keyword}, '%') or M_B_BRANCH like concat('%',
				#{keyword}, '%'))</if>
		</if>
	</sql>
	<select id="matchtotalcount" parameterType="TeamPageDto"
		resultType="int">
		select count(*) from matchboard_tb where M_B_NO > 0
		<include refid="searchCondition_match"></include>
	</select>
	<select id="getmatchlist" parameterType="TeamPageDto"
		resultType="MatchlistDto">
		select M_B_NO, M_B_NAME, M_B_BRANCH BRANCH_NO, M_B_GROUND, M_B_TEL,
		M_B_SCHEDULE, M_B_TIME, M_B_STATUS, M_B_LEVEL, M_B_CONTENTS, a.ID,
		M_B_TYPE, M_B_COUNT, b.BRANCH_NAME M_B_BRANCH
		from matchboard_tb a
		inner join branch_tb b on a.M_B_BRANCH = b.BRANCH_NO
		where M_B_NO > 0
		<include refid="searchCondition_match"></include>
		order by M_B_NO desc limit #{page}, 9
	</select>

