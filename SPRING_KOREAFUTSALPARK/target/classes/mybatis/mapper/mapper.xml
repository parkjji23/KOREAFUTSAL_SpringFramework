	<select id="listPage" parameterType="TeamPageDto"
		resultType="TeamBoardListDto">
		select * from teamboard_tb where NO > 0
		<include refid="teamSearchCondition"></include>
		<include refid="searchCondition"></include>
		order by NO desc, DATE desc limit #{PAGE}, 9
	</select>
	<!-- 검색 조건에 해당하는 전체 게시물 수 조회 -->
	<sql id="searchCondition">
		<if test="SEARCHTYPE != null">
			<if test="SEARCHTYPE == 'subject'.toString()">and SUBJECT like concat('%', #{keyword}, '%')</if>
			<if test="SEARCHTYPE == 'id'.toString()">and ID like concat('%', #{keyword}, '%')</if>
			<if test="SEARCHTYPE == 'all'.toString()">and (SUBJECT like concat('%', #{keyword}, '%') or ID
				like concat('%', #{keyword}, '%'))</if>
		</if>
	</sql>
	<sql id="teamSearchCondition">
		<if test="TEAMCODE != null">and TEAMCODE = #{TEAMCODE}</if>
	</sql>
	<select id="gettotalcount" parameterType="TeamPageDto"
		resultType="int">
		select count(*) from teamboard_tb where NO > 0
		<include refid="teamSearchCondition"></include>
		<include refid="searchCondition"></include>
	</select>
	<!-- 팀리스트 검색 페이징 추가 -->
	<sql id="searchCondition_teamlist">
		<if test="SEARCHTYPE != null">
			<if test="SEARCHTYPE == 'branch'.toString()">and TEAMCLASS like concat('%', #{keyword}, '%')</if>
			<if test="SEARCHTYPE == 'name'.toString()">and TEAMNAME like concat('%', #{keyword}, '%')</if>
			<if test="SEARCHTYPE == 'all'.toString()">and (TEAMCLASS like concat('%', #{keyword}, '%') or TEAMNAME
				like concat('%', #{keyword}, '%'))</if>
		</if>
	</sql>
	<select id="getteamlist" parameterType="TeamPageDto"
		resultType="TeamDto">
		select * from teaminfo_tb where TEAMCODE > 0
		<include refid="searchCondition_teamlist"></include>
		order by TEAMBIRTH asc limit #{PAGE}, 9
	</select>
